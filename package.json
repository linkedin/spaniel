{
  "name": "spaniel",
  "version": "3.3.1",
  "description": "LinkedIn's viewport tracking library",
  "license": "Apache 2.0",
  "main": "exports/spaniel.js",
  "scripts": {
    "test": "tsc && npm run build && testem ci && node test/headless/run",
    "test:headless": "mocha --compilers js:babel-core/register test/headless/specs/**/*.js",
    "serve": "node test/headless/server/app",
    "watch": "broccoli-timepiece exports",
    "build": "./scripts/build.sh",
    "stats": "node scripts/size-calc",
    "prepublish": "npm run build",
    "docs:build": "./scripts/build-docs.sh",
    "docs:publish": "./scripts/publish-docs.sh"
  },
  "files": [
    "src",
    "exports",
    ".babelrc",
    "Brocfile.js",
    "build.sh",
    "README.md",
    "rollup.config.js",
    "declarations.d.ts",
    "CHANGELOG.md",
    ".travis.yml"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/linkedin/spaniel"
  },
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": {
    "name": "Asa Kusuma"
  },
  "homepage": "https://github.com/linkedin/spaniel",
  "jsnext:main": "exports/es6/index.js",
  "module": "exports/es6/index.js",
  "typings": "exports/es6/index.d.ts",
  "devDependencies": {
    "assert": "^1.3.0",
    "babel-core": "^6.11.4",
    "babel-preset-env": "^1.6.1",
    "broccoli": "^1.1.0",
    "broccoli-cli": "1.0.0",
    "broccoli-funnel": "^1.2.0",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-rollup": "^1.3.0",
    "broccoli-string-replace": "^0.1.1",
    "broccoli-typescript-compiler": "^1.0.1",
    "broccoli-uglify-sourcemap": "^1.4.2",
    "chai": "^3.5.0",
    "chokidar": "^1.6.0",
    "express": "^4.13.4",
    "gzip-size": "^3.0.0",
    "mocha": "^2.4.5",
    "nightmare": "^3.0.0",
    "pretty-bytes": "^4.0.2",
    "rsvp": "^3.1.0",
    "serve-static": "^1.10.2",
    "sinon": "^1.14.1",
    "testem": "^1.12.0",
    "tslint": "^3.6.0",
    "typescript": "2.5.3"
  }
}
